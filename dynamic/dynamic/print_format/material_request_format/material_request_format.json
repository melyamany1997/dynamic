 "doctype": "Print Format",
 "font": "Default",
 "html": "\n\n {%- set company = frappe.get_doc(\"Company\", doc.company) -%}\n <div class=\"mb-4\">\n      <div class=\"col-xs-12\" style=\"text-align: right\">\n        <img src=\"{{company.company_logo}}\" width=\"20%\" alt=\"\" />\n      </div>\n    </div>\n    <br/>\n    <br/>\n        \n    <div class=\"row\">\n        <div class=\"col-xs-4\">\n            <div ><strong>&nbsp; {{_(\"purpose\")}}&nbsp; : &nbsp; </strong>&nbsp; <strong> {{_(\"Material Request\")}} </strong></div>\n        </div>\n    \n        <div class=\"col-xs-6\">\n            <div dir=\"auto\"><strong>{{_(\"Doc NO\")}} &nbsp;:&nbsp; </strong>&nbsp; <strong>{{doc.name}}</strong></div>\n        </div>\n        \n         \n  \n    </div>\n    <div class=\"row mt-2\">\n        <div class=\"col-xs-12\">\n            <div dir=\"auto\"><strong>{{_(\"Transaction Date And Time\")}} : </strong>{{doc.posting_date}}</div>\n        </div>\n    </div>\n\n  \n     <div class=\"row mt-2\">\n        <div class=\"col-xs-6\">\n            <div class=\"green\"><strong>{{_(\"From Warhouse\")}} : </strong>{{doc.set_from_warehouse}}</div>\n        </div>\n    \n        <div class=\"col-xs-6\">\n            <div class=\"blue\"><strong>{{_(\"Target Warhouse\")}} : </strong>{{doc.set_warehouse}}</div>\n        </div>\n        \n    </div>\n    \n    \n    <table class=\"table table-bordered table-condensed\">\n      <thead>\n          <tr>\n          <td scope=\"col\"><strong>{{_(\"Number\")}}</strong></td>\n          <td scope=\"col\"><strong>{{_(\"Item Code\")}}</strong></td>\n          <td scope=\"col\"><strong>{{_(\"Item Name\")}}</strong></td>\n          <td scope=\"col\"><strong>{{_(\"Quantity\")}}</strong></td>\n        </tr>\n        {%for row in doc.items%}\n        <tr>\n            <td><strong>{{row.idx}}</strong></td>\n            <td>{{row.item_code}}</td>\n            <td>{{row.item_name}}</td>\n             <td>{{row.qty}}</td>\n        </tr>\n        \n        {%endfor%}\n      </thead>\n    </table>\n    \n\n    <div class=\"row margin-bottom\">\n        <div class=\"col-xs-6\" style=\"text-align: center\">\n        <strong>{{_(\"Storekeeper's signature\")}} :</strong>\n        <br />\n        <br />\n        <strong>{{_(\"...................................\")}}</strong>\n      </div>\n      <div class=\"col-xs-6\" style=\"text-align: center\">\n        <strong>{{_(\"The recipient's signature\")}} : </strong>\n        <br />\n        <br />\n        <strong>{{_(\"...................................\")}}</strong>\n      </div>\n    \n      \n\n    </div>\n\n\n\n\n\n\n\n\n\n\n",
