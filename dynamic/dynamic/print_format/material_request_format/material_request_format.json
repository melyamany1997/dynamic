 "css": "\n\n\n.grid-container {\n    \n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    grid-gap: 20px;\n}\n\n.grid-container_2{\n    margin-top:7px;\n    display: grid;\n    grid-template-columns: 1fr 1fr ;\n    grid-gap: 20px;\n}\n.row{\n    margin-top:2em;\n}\n.table{\n    margin-top:4em;\n}\nstrong{\n    font-weight:bold;\n  font-size:14px;\n}\ntd{\n   font-weight: 700;\n}",
