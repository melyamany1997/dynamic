 "html": "<!-- {%- from \"templates/print_formats/standard_macros.html\" import add_header -%} -->\n\n<!--{% set customer_doc = frappe.get_doc(\"Customer\",doc.customer)%}-->\n<!--{% if doc.contact_person%}-->\n<!--{% set cst_contact = frappe.get_doc(\"Contact\",doc.contact_person) %}-->\n<!--{%else%}-->\n<!--{% set cst_contact = 0 %}-->\n<!--{%endif%}-->\n\n\n\n<!--{% if letter_head and not no_letterhead -%}-->\n<!--{% if print_settings.repeat_header_footer %}-->\n<!--   <br>  <div class=\"letter-head\">{{ letter_head }}</div>-->\n<!--{%- endif %}{%- endif %}-->\n\n\n<div class=\"container-fluid\" dir=\"auto\">\n    <!-- header -->\n    <div class=\"row\">\n                <div class=\"col-sm-12 col-xs-12\" style=\"text-align: right;font-size: 17px;\"><b>{{_(\"\u0634\u0631\u0643\u0629 \u0641\u064a\u0648\u062a\u0634\u0631 \u0648\u0648\u062f \u0644\u062a\u062c\u0627\u0631\u0629 \u0648\u062a\u0635\u0646\u064a\u0639 \u0627\u0644\u0623\u062e\u0634\u0627\u0628\")}}</b></div>\n        </div>\n    <div class=\"row mt-5\">\n        <div class=\"col-sm-5 col-xs-5\" style=\"text-align: right;font-size: 17px;\"><b>\n            {%-if doc.payment_type== '\u062a\u0633\u0644\u0645' or doc.payment_type== 'Recieve' -%}\n            {{_(\"\u0623\u0630\u0646 \u0627\u0633\u062a\u0644\u0627\u0645 \")}}\n            {%else%}\n            {{_(\"\u0623\u0630\u0646 \u0635\u0631\u0641\")}}\n            {%-endif-%}\n        </b></div>\n        <div class=\"col-sm-3 col-xs-3\" style=\"text-align: center;\"><b>{{doc.name}}</b></div>\n                \n        <div class=\"col-sm-4 col-xs-4 mt-1\" style=\"\"> \n            <img  src=\"/assets/dynamic/images/future.jpeg\" width=\"300\" height=\"100\">\n        </div>\n        \n    </div>\n    <div class=\"row\">\n        <div class=\"col-sm-8 col-xs-8\" style=\"text-align: right;font-size: 17px;\"></div>\n        <div class=\"col-sm-2 col-xs-2\" style=\"text-align: right;font-size: 17px;\">{{_(\"\u0642\u0631\u0634\")}}</div>\n        <div class=\"col-sm-2 col-xs-2\" style=\"text-align: right;font-size: 17px;\">{{_(\"\u062c\u0646\u064a\u0647\")}}</div>\n    </div>\n\n    <!-- second row -->\n    <div class=\"row mt-1\">\n        <div class=\"col-sm-3 col-xs-3  mt-1\"><b>{{_(\"\u0627\u0644\u062a\u0627\u0631\u064a\u062e\")}}</b></div>\n        <div class=\"col-sm-5 col-xs-5  mt-1\" style=\"text-align: right;\">\n            <b style=\"font-weight: bold;font-size: 18px;\">{{doc.posting_date}}</b>\n            </div>\n        <div class=\"col-sm-4 col-xs-4  mt-1\"  style=\"text-align: center;font-size: 15px;padding-left: 50px;\">{{doc.paid_amount}}</div>\n    </div>\n\n    <!-- third rowstyle=\"background-color: #b3b3b3;\" -->\n    <div class=\"row mt-1\">\n        <div class=\"col-sm-3 col-xs-3  mt-1\"><b>{{_(\"\u0623\u0633\u062a\u0644\u0645\u0646\u0627 \u0645\u0646 \u0627\u0644\u0633\u064a\u062f\")}}</b></div>\n        <div class=\"col-sm-5 col-xs-5  mt-1\" style=\"text-align: right;\">\n            <b style=\"font-weight: bold;font-size: 18px;\">{{doc.party_name}}</b>\n            </div>\n        <div class=\"col-sm-4 col-xs-4  mt-1\"  style=\"text-align: center;font-size: 15px;padding-left: 50px;\"></div>\n    </div>\n\n     <!-- four row -->\n     <div class=\"row mt-1\">\n        <div class=\"col-sm-3 col-xs-3  mt-1\"><b>{{_(\" \u0645\u0628\u0644\u063a \u0648\u0642\u062f\u0631\u0629 \")}}</b></div>\n        <div class=\"col-sm-5 col-xs-5  mt-1\" style=\"text-align: right;\">\n            <b style=\"font-weight: bold;font-size: 14px;\">{{frappe.utils.money_in_words(doc.paid_amount,doc.currency) }}\n            </b>\n            </div>\n        <div class=\"col-sm-4 col-xs-4  mt-1\"  style=\"text-align: center;font-size: 15px;padding-left: 50px;\"></div>\n    </div>\n\n    <!-- four row -->\n    <div class=\"row mt-1\">\n        <div class=\"col-sm-5 col-xs-5  mt-1\" style=\"text-align: right;\">\n        </div>\n        <div class=\"col-sm-4 col-xs-4  mt-1\"  style=\"text-align: center;font-size: 15px;padding-left: 50px;\"></div>\n        <div class=\"col-sm-3 col-xs-3  mt-1\"><b>{{_(\"  \u0627\u0644\u0645\u0633\u062a\u0644\u0645 \")}}</b></div>\n    </div>\n    <!-- four row ////-->\n    <div class=\"row mt-1\">\n        <div class=\"col-sm-5 col-xs-5  mt-1\" style=\"text-align: right;\">\n        </div>\n        <div class=\"col-sm-4 col-xs-4  mt-1\"  style=\"text-align: center;font-size: 15px;padding-left: 50px;\"></div>\n        <div class=\"col-sm-3 col-xs-3  mt-1\"><b>{{\".....\" * 7}}</b></div>\n    </div>\n\n\n     \n\n\n\n</div>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
