 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header -%}\n<style>\n\n</style>\n<div>\n    <div id=\"header-html\" class=\"hidden-pdf\">\n            <img src=\"/assets/dynamic/images/hm_header.jpg\" />\n    </div>\n    <div class=\"row margin-bottom\">\n        <div class=\"col-xs-3\" style=\"text-align: center;\">\n            <div>\n                <div class=\"col-xs-4\" style=\"text-align: center;\">\n                    <div class=\"row\">\n                        <h4 class=\"cent-egp\">\u0642\u0631\u0634</h4>\n                    </div>\n                    <div class=\"row \">\n                        <div class=\"cent-egp box\">{{frappe.utils.cint((doc.amount % 1)*100)}}</div>\n                    </div>\n                </div>\n                <div class=\"col-xs-4\" style=\"text-align: center;\">\n                    <div class=\"row\">\n                        <h4 class=\"amount-egp\">\u062c\u0646\u064a\u0629</h4>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"amount-egp box\">{{frappe.utils.cint(doc.amount)}}</div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\" style=\"text-align: center;\">\n            <div class=\"center\" style=\"text-align: center;\">\n                <h4 class=\"header\">\n                    <div>\u0633\u0646\u062f \u0642\u0628\u0640\u0640\u0640\u0640\u0640\u0636</div>\n                    <div class=\"underline\">Receipt Voucher</div>\n                </h4>\n            </div>\n        </div>\n    </div>\n    <div class=\"row margin-bottom\" style=\"text-align:center\">\n        <div class=\"col-xs-4\">\n            <div>\n                \u0627\u0644\u062a\u0627\u0631\u064a\u062e {{get_hijri_date(doc.posting_date)}} \u0647\u0640\n            </div>\n        </div>\n        <div class=\"col-xs-4\">\n            <h3 class=\"serial\">{{doc.name}}</h3>\n        </div>\n        <div class=\"col-xs-4\">\n            \u0627\u0644\u0645\u0648\u0627\u0641\u0642 {{doc.posting_date}} \u0645\u0640\n        </div>\n    </div>\n\n    <div class=\"row margin-bottom\" style=\"direction:ltr\">\n        <table width=\"95%\" style=\"margin: auto;\">\n            <tr>\n                <td style=\"text-align: left;\" colspan=\"2\"><strong>Received From Mrs : </strong></td>\n                <td style=\"text-align: center;\" colspan=\"2\">{{ doc.accounts[0].party }}</td>\n                <td style=\"text-align: right;\" colspan=\"2\"><strong>: \u0627\u0633\u062a\u0644\u0645\u0646\u0627 \u0645\u0646 \u0627\u0644\u0633\u064a\u062f / \u0627\u0644\u0633\u0627\u062f\u0629</strong></td>\n            </tr>\n            <tr>\n                <td style=\"text-align: left;\" colspan=\"2\"><strong>Amount : </strong></td>\n                <td style=\"text-align: center;\" colspan=\"2\">{{ frappe.utils.money_in_words(doc.amount,doc.currency) }}\n                </td>\n                <td style=\"text-align: right;\" colspan=\"2\"><strong>: \u0645\u0628\u0644\u063a \u0648 \u0642\u062f\u0631\u0647</strong></td>\n            </tr>\n            <tr>\n                <td style=\"text-align: left;\"><strong>Bank : </strong></td>\n                <td style=\"text-align: center;\">{{ doc.account or \"\"}}</td>\n                <td style=\"text-align: right;\"><strong>: \u0639\u0644\u064a \u0628\u0646\u0643</strong></td>\n\n                <td style=\"text-align: left;\"><strong>Cash/Cheque No. : </strong></td>\n                <td style=\"text-align: center;\">{{ doc.reference_no or \"\" }}</td>\n                <td style=\"text-align: right;\"><strong>: \u0646\u0642\u062f\u0627 / \u0634\u064a\u0643 \u0631\u0642\u0645</strong></td>\n            </tr>\n            <tr>\n                <td style=\"text-align: left;\" colspan=\"2\"><strong>Being : </strong></td>\n                <td style=\"text-align: center;\" colspan=\"2\">{{ doc.notes or \"\" }}</td>\n                <td style=\"text-align: right;\" colspan=\"2\"><strong>: \u0648\u0630\u0644\u0643 \u0645\u0642\u0627\u0628\u0644</strong></td>\n            </tr>\n        </table>\n    </div>\n\n    <br>\n    <div class=\"row margin-bottom\" style=\"text-align: center;\">\n        <div class=\"col-xs-4\">\n            <h4>\u0627\u0644\u0645\u062d\u0627\u0633\u0628 Accountant</h4>\n            <h4>.........................</h4>\n        </div>\n        <div class=\"col-xs-4\">\n            <h4>\u0627\u0644\u0645\u0633\u062a\u0644\u0645 Receiver</h4>\n            <h4>.........................</h4>\n        </div>\n        <div class=\"col-xs-4\">\n            <h4>\u0627\u0644\u062e\u062a\u0645 Seal</h4>\n            <h4>.........................</h4>\n        </div>\n    </div>\n    <br>\n\n    <div id=\"footer-html\" class=\"\">\n        <img src=\"/assets/dynamic/images/hm_footer.jpg\" />\n    </div>\n\n</div>",
